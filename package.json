{
  "name": "unocss-preset-glyph",
  "version": "0.2.2",
  "description": "UnoCSS preset for embedding subset of glyphs from fonts",
  "engines": {
    "node": ">=14.18"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-demo": "vite build --mode demo",
    "test": "vitest",
    "test:update": "vitest -u",
    "release": "npx bumpp package.json --commit \"release: v\" --push --tag"
  },
  "devDependencies": {
    "@types/estree": "^1.0.0",
    "@types/node": "^18.14.6",
    "fontmin": "^0.9.9",
    "inconsolata": "^0.0.2",
    "inter-ui": "^3.19.3",
    "marked": "^4.2.12",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "wawoff2": "^2.0.1",
    "woff2sfnt-sfnt2woff": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chu121su12/unocss-preset-glyph.git"
  },
  "keywords": [
    "unocss",
    "fontmin",
    "font subset"
  ],
  "author": "Saya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chu121su12/unocss-preset-glyph/issues"
  },
  "homepage": "https://github.com/chu121su12/unocss-preset-glyph"
}
